    <selectionEntry id="52b5-b14e-d2d6-1249" name="War Wagon" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="increment" field="544b-c682-299e-3bff" value="1.0">
          <repeats>
